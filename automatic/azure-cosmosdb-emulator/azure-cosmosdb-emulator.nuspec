    <authors>Microsoft</authors>
    <projectUrl>https://cosmosdb.com</projectUrl>
    <iconUrl>http://cdn.rawgit.com/ryancrawcour/chocolatey-packages/master/icons/azure-documentdb.png</iconUrl>
